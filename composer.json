{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org"
    }
  ],
  "name": "mselbach/static-info-tables-ua",
  "description": "(Ukrainian) language pack for the Static Info Tables providing localized names for countries,currencies and so on.",
  "keywords": [
    "TYPO3 CMS",
    "Static Info Tables"
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Manuel Selbach",
      "role": "Developer"
    }
  ],
  "replace": {
    "static_info_tables_ua": "self.version",
    "typo3-ter/static-info-tables-ua": "self.version"
  },
  "require": {
    "php": ">=5.6",
    "typo3/cms-core": "^7.6 || ^8.7",
    "sjbr/static-info-tables": "^6.5"
  },
  "require-dev": {
    "namelesscoder/typo3-repository-client": "^1.3",
    "squizlabs/php_codesniffer": "^2.6",
    "phpunit/phpunit": "^5.6",
    "nimut/testing-framework": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Mselbach\\StaticInfoTablesUa\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mselbach\\StaticInfoTablesUa\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
